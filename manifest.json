{
  "manifest_version": 2,
  "name": "LangSure",
  "version": "1.0.1",
  "description": "lightweight add-on that gives auditory feedback when switching languages.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "Assets/1.mp3"
  ],
  "icons": {
    "256": "Assets/1.png"
  }
}
